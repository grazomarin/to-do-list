{
	"name": "to-do-list",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/grazomarin/to-do-list.git"
	},
	"author": "",
	"license": "ISC",
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest",
			"eslint:recommended"
		],
		"rules": {
			"semi": [
				1,
				"always"
			],
			"indent": [
				1,
				"tab"
			],
			"linebreak-style": [
				0,
				"unix"
			],
			"no-use-before-define": [
				1,
				{
					"functions": false
				}
			],
			"no-unused-expressions": [
				0,
				{
					"allowTernary": true
				}
			],
			"max-len": [
				2,
				{
					"code": 80
				}
			]
		}
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"dependencies": {
		"moment": "^2.29.4",
		"pikaday": "^1.8.2",
		"react": "^18.2.0",
		"react-date-picker": "^9.2.0",
		"react-datepicker": "^4.10.0",
		"react-dom": "^18.2.0",
		"uniqid": "^5.4.0",
		"webfontloader": "^1.6.28"
	},
	"devDependencies": {
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10",
		"@vitejs/plugin-react": "^3.1.0",
		"@vitejs/plugin-react-refresh": "^1.3.6",
		"css-loader": "^6.7.1",
		"eslint": "^8.35.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.26.0",
		"prettier": "^2.7.1",
		"sass": "^1.55.0",
		"sass-loader": "^13.0.2",
		"style-loader": "^3.3.1",
		"vite": "^4.1.4"
	}
}
